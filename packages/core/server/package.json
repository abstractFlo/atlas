  "homepage": "",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.esm.mjs"
  },
  "scripts": {
    "prebuild": "yarn clean",
    "build": "rollup -c",
    "postbuild": "yarn types && rimraf ./dist/packages && yarn local:pack",
    "prewatch": "yarn clean",
    "watch": "rollup -cw",
    "clean": "rimraf {dist,build,types,*.d.ts,*.tgz}",
    "types": "api-extractor run --local --verbose",
    "local:pack": "yarn pack --filename server.tgz"
  },
  "peerDependencies": {
    "@abstractflo/atlas-shared": "^3.1.0",
    "@altv/types-server": "^2.0.0"
  },
  "devDependencies": {
    "@abstractflo/atlas-shared": "^3.1.0-next.8",
    "@altv/types-server": "*"
  },
  "dependencies": {
    "dotenv": "^10.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
