{
  "name": "@abstractflo/atlas-shared",
  "version": "3.1.0",
  "description": "> TODO: description",
  "author": "Florian Uhlrich <f.uhlrich@abstractmedia.de>",
  "homepage": "",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.esm.js"
  },
  "scripts": {
    "prebuild": "yarn clean",
    "build": "microbundle --no-sourcemap --format=cjs,esm --no-compress",
    "postbuild": "yarn types && rimraf ./dist/packages && yarn local:pack",
    "prewatch": "yarn clean",
    "watch": "microbundle watch --no-sourcemap --format=cjs,esm --no-compress",
    "clean": "rimraf {dist,build,types,*.d.ts,*.tgz}",
    "types": "api-extractor run --local --verbose",
    "local:pack": "yarn pack --filename shared.tgz"
  },
  "dependencies": {
    "@abraham/reflection": "^0.8.0",
    "rxjs": "^7.2.0",
    "tslib": "^2.3.0",
    "tsyringe": "^4.6.0",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "@altv/types-shared": "^1.0.1"
  },
  "devDependencies": {
    "@altv/types-shared": "*"
  }
}
