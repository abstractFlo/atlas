    "@swc/core": "^1.2.64",
    "cfg-reader": "^2.0.0",
    "change-case": "^4.1.2",
    "colorette": "^1.2.2",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "execa": "^5.1.1",
    "fs-jetpack": "^4.1.0",
    "import-meta-resolve": "^1.1.1",
    "inquirer": "^8.1.2",
    "js-yaml": "^4.1.0",
    "jscodeshift": "^0.13.0",
    "lodash": "^4.17.21",
    "progress": "^2.0.3",
